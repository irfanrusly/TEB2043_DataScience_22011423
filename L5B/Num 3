# Input an integer
num <- 1634
temp <- num
sum_val <- 0

# Determine the number of digits (n)
n <- nchar(as.character(num))

# Calculate the sum of digits raised to the power of n
while (temp > 0) {
  digit <- temp %% 10
  sum_val <- sum_val + (digit ^ n)
  temp <- floor(temp / 10)
}

# Check if sum matches the original number
if (num == sum_val) {
  cat(num, "is an Armstrong number.\n")
} else {
  cat(num, "is not an Armstrong number.\n")
}
